<h2><%= @artist.name %></h2>

<h3>Genres:</h3>
<ul>
<% @artist.genres.each do |genre| %>
	<li><h4><a href='/genres/<%=genre.slug%>'><%=genre.name%></a></h4></li>
<% end %>
</ul>


<h3>Songs</h3>
<ul>
<% @artist.songs.each do |song| %>
<li><h4><a href='/songs/<%=song.slug%>'><%=song.name%></a></h4></li>
<%end %>
</ul>



